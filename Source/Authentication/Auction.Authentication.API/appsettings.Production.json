{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MySql": ""
  },
  "Jwt": {
    "Secret": "",
    "Expiry": "",
    "Audience": "",
    "Issuer": "",
    "RefreshTokenSecret": "",
    "RefreshTokenExpiry": "",
    "RefreshTokenAudience": "",
    "RefreshTokenIssuer": ""
  },
  "RabbitMq": {
    "Host": "",
    "Port": "",
    "Username": "",
    "Password": "",
    "QueueName": ""
  }
}
